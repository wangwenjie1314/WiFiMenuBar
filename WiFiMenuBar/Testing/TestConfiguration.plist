<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>TestSettings</key>
    <dict>
        <key>EnableDebugMode</key>
        <true/>
        <key>ShowTestMenuInRelease</key>
        <false/>
        <key>AutoRunTestsOnLaunch</key>
        <false/>
        <key>SaveTestReportsAutomatically</key>
        <true/>
    </dict>
    <key>IntegrationTestSettings</key>
    <dict>
        <key>TestTimeout</key>
        <integer>30</integer>
        <key>MaxRetryAttempts</key>
        <integer>3</integer>
        <key>TestDataPath</key>
        <string>TestData</string>
    </dict>
    <key>PerformanceTestSettings</key>
    <dict>
        <key>MemoryThreshold</key>
        <real>100.0</real>
        <key>CPUThreshold</key>
        <real>10.0</real>
        <key>ResponseTimeThreshold</key>
        <real>0.1</real>
    </dict>
    <key>UserExperienceTestSettings</key>
    <dict>
        <key>MinimumResponseTime</key>
        <real>0.05</real>
        <key>MaximumResponseTime</key>
        <real>0.2</real>
        <key>TestAccessibility</key>
        <true/>
        <key>TestKeyboardNavigation</key>
        <true/>
    </dict>
    <key>StabilityTestSettings</key>
    <dict>
        <key>MinimumStabilityScore</key>
        <real>70.0</real>
        <key>MaximumCrashCount</key>
        <integer>0</integer>
        <key>MaximumExceptionCount</key>
        <integer>5</integer>
    </dict>
    <key>IconTestSettings</key>
    <dict>
        <key>TestAllSizes</key>
        <true/>
        <key>TestAllThemes</key>
        <true/>
        <key>TestAnimations</key>
        <true/>
        <key>RequiredIconStates</key>
        <array>
            <string>connected</string>
            <string>disconnected</string>
            <string>connecting</string>
            <string>error</string>
        </array>
    </dict>
    <key>ReportSettings</key>
    <dict>
        <key>ReportFormat</key>
        <string>text</string>
        <key>IncludeDetailedLogs</key>
        <true/>
        <key>IncludeScreenshots</key>
        <false/>
        <key>AutoOpenReportAfterTest</key>
        <true/>
    </dict>
</dict>
</plist>